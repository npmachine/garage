{
  "name": "app",
  "private": true,
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "esbuild": "^0.20.2",
    "postcss": "^8.4.38",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets",
